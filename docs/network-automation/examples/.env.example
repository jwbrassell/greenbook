# Network Device Credentials
NETWORK_USER=admin
NETWORK_PASSWORD=your_secure_password_here
NETWORK_ENABLE=your_enable_password_here

# API Settings
API_TIMEOUT=30
API_RETRIES=3

# SSL Verification
# Set to 'true' in production
SSL_VERIFY=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=network_automation.log

# Optional: Proxy Configuration
# HTTP_PROXY=http://proxy.example.com:8080
# HTTPS_PROXY=https://proxy.example.com:8080
